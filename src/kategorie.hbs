<!DOCTYPE html>
<html lang="en">
  <head>
    {{> head }}
  <head>

  <body>
    <div class="holder">
      {{> notification }}
      {{> header }}
      {{> menu }}
    </div>

    <main>
      <section>
        <div class="wrapper category clear">
          <nav class="sitebar left">
            {{> sitebar-menu }}
          </nav>
          <div class="content right">
            <div class="controlls text clear">
              <div class="products-amount left">
                Zeige 1-12 von 36 Produkten
              </div>
              <div class="pagination right">
                Seite:
                <span class="page active">
                  <a href="#">1</a>
                </span>
                <span class="page">
                  <a href="#">2</a>
                </span>
                <span class="page">
                  <a href="#">3</a>
                </span>
              </div>
              <div class="ordering right">
                Sortierung nach:
                <select class="select">
                  <option>beliebt</option>
                </select>
              </div>
            </div>
            <div class="heading-1">Tee</div>

            <div class="products">
              <div class="product">
                <div class="category-slider swiper-container">
                  <ul class="swiper-wrapper">
                    <li class="swiper-slide">
                      <img src="img/category/product-01.jpg" class="picture e" alt="" />
                    </li>
                    <li class="swiper-slide">
                      <img src="img/category/product-01.jpg" class="picture e" alt="" />
                    </li>
                  </ul>
                </div>
                <div class="title">Lemon-Sizilia</div>
                <div class="rating">
            			<span>☆</span>
                  <span>☆</span>
                  <span>☆</span>
                  <span>☆</span>
                  <span>☆</span>
            		</div>
                <div class="price-wrapper">
                  <div class="row">
                    <span class="row-title">T-Preis:</span> <span class="new">3.90€</span> inkl. MwSt.</span>
                  </div>
                  <div class="row">
                    <span class="row-title underline">C-Preis:</span> <span class="new">2.53€</span> inkl. MwSt.</span>
                  </div>
                </div>
              </div>
              <div class="product">
                <span class="label">NEU</span>
                <img src="img/category/product-02.jpg" class="picture" alt="" />
                <div class="title">Black Currant ST</div>
                <div class="rating">
            			<span>☆</span>
                  <span>☆</span>
                  <span>☆</span>
                  <span>☆</span>
                  <span>☆</span>
            		</div>
                <div class="price-wrapper">
                  <div class="row">
                    <span class="row-title">T-Preis:</span> <span class="new">4.20€</span> inkl. MwSt.</span>
                  </div>
                  <div class="row">
                    <span class="row-title underline">C-Preis:</span> <span class="new">2.73€</span> inkl. MwSt.</span>
                  </div>
                </div>
              </div>
              <div class="product">
                <span class="label bg-brown">%</span>
                <img src="img/category/product-03.jpg" class="picture" alt="" />
                <div class="title">Erdbeere-Sahne ST</div>
                <div class="rating">
            			<span>☆</span>
                  <span>☆</span>
                  <span>☆</span>
                  <span>☆</span>
                  <span>☆</span>
            		</div>
                <div class="price-wrapper">
                  <div class="row">
                    <span class="row-title">T-Preis:</span> <span class="new">3.99€</span> inkl. MwSt.</span>
                  </div>
                  <div class="row">
                    <span class="row-title underline">C-Preis:</span> <span class="new">2.55€</span> inkl. MwSt.</span>
                  </div>
                </div>
              </div>
              <div class="product">
                <span class="label">-5<i class="fa fa-euro"></i></span>
                <img src="img/homepage/current-offers/offer-01.jpg" class="picture" alt="" />
                <div class="title">Erdbeere-Sahne ST</div>
                <div class="rating">
            			<span>☆</span>
                  <span>☆</span>
                  <span>☆</span>
                  <span>☆</span>
                  <span>☆</span>
            		</div>
                <div class="price-wrapper">
                  <div class="row">
                    <span class="row-title">T-Preis:</span> <span class="new">3.99€</span> inkl. MwSt.</span>
                  </div>
                  <div class="row">
                    <span class="row-title underline">C-Preis:</span> <span class="new">2.55€</span> inkl. MwSt.</span>
                  </div>
                </div>
              </div>
            </div>
            {{> club-banner }}
          </div>
        </div>
      </section>

      <section>
        {{> advertising-services }}
      </section>

    </main>

    {{> footer }}
    {{> copyright }}
  </body>
</html>
